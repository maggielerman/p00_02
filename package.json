{
  "name": "magg1e.com",
  "version": "0.1.0",
  "description": "portfolio",
  "author": "Maggie Lerman",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/maggielerman/011721"
  },
  "scripts": {
    "fetchData": "src/fetchData.sh",
    "fetchData2": "src/fetchData_sft.sh",
    "build:dev": "ELEVENTY_ENV=development eleventy",
    "build": "npm run build:tailwind && npm run build:html",
    "build:css": "postcss src/site/assets/css/index.css -o src/site/assets/css/main.css",
    "build:tailwind": "npx tailwindcss -i src/site/assets/css/styles/production.css -o src/site/assets/css/main.css",
    "build:html": "eleventy",
    "watch:dev": "ELEVENTY_ENV=development eleventy --serve --port=3000",
    "watch": "npm-run-all --parallel watch:html",
    "watch:html": "eleventy --serve --port=3000",
    "start": "npm run watch",
    "clean": "rimraf dist",
    "killall": "killall node",
    "kill3000": "npx kill-port 3000",
    "debug": "DEBUG=* eleventy"
  },
  "dependencies": {
    "@11ty/eleventy-cache-assets": "^2.1.0",
    "@11ty/eleventy-img": "^0.7.8",
    "@11ty/eleventy-plugin-rss": "^1.1.0",
    "@fullhuman/postcss-purgecss": "^4.0.3",
    "@lottiefiles/lottie-interactivity": "^0.1.3",
    "@sir_koty/json-extractor": "^1.0.1",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/typography": "^0.4.1",
    "axios": "^0.21.1",
    "bash": "^0.0.1",
    "critical": "^4.0.1",
    "cssnano": "^4.1.11",
    "eleventy-plugin-purgecss": "^0.1.1",
    "engine.io-client": "^3.5.1",
    "fs": "^0.0.1-security",
    "html-minifier": "^4.0.0",
    "mime": "^1.4.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.1",
    "rimraf": "^3.0.2",
    "stripe": "^8.140.0",
    "superagent": "^6.1.0",
    "svg-sprite": "^1.5.0",
    "uglify-js": "^3.12.8",
    "vinyl": "^2.2.1",
    "wget": "0.0.1"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "autoprefixer": "^10.3.7",
    "cross-env": "^5.2.1",
    "postcss": "^8.3.9",
    "tailwindcss": "^2.2.16"
  }
}
