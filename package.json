    "fs": "^0.0.1-security",
    "html-minifier": "^4.0.0",
    "mime": "^1.4.1",
    "normalize.css": "^8.0.1",
    "npm": "^8.5.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^9.0.1",
    "postcss-import": "^14.0.2",
