<div class="grid grid-cols-2 gap-2 md:gap-6 p-2 md:p-6">
 

  <section class="col-span-1">
    <div x-data="{open: false}">
    
        <div class="cursor-pointer object-cover h-full " @click="open =! open" aria-label="Modal"> 
            {% png "src/site/images/logo-branding/case-study/bar-cart-love/bcl01b.png", "bar cart love ", ' 50vw, 100vw', 'h-full w-full object-cover rounded-lg' %}
        </div>
        

        <div x-show.transition.slide="open" x-transition x-transition.duration.500ms role="gallery" aria-modal="true"
            class="modal bg-black bg-opacity-50 p-4 md:p-8 text-left ease-in-out duration-300 fixed top-0 right-0 w-screen h-screen flex place-content-center">
            <div class="bg-blue-500 max-w-full max-h-full grid grid-cols-1 overflow-hidden relative"
                @mousedown.away="open = false" @keydown.window.escape="open = false" >
  
                  <button class="absolute top-0 right-0 cursor-pointer py-2 px-4 hover:opacity-70"  @click="open =! open" @mousedown.away="open = false" @keydown.window.escape="open = false">
                    <i class="fa-solid fa-xmark"></i> 
                  </button>
           
